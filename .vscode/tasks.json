{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "test_polibase_project",
            "type": "shell",
            "group": "test",
            "options": {
                "cwd": "${workspaceFolder}/tests"
            },
            "command": "poetry",
            "args": [
                "run",
                "pytest"
            ],
        },
        {
            "label": "run_minutes_divide_processor",
            "type": "shell",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "command": "poetry",
            "args": [
                "run",
                "python",
                "src/main.py"
            ],
        }
    ]
}
