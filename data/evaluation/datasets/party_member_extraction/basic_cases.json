      "id": "pme_001",
      "description": "Simple table-based member list extraction",
      "input": {
        "pages": [
          {
            "url": "https://example-party.jp/members",
            "html_content": "<html><body><h1>議員一覧</h1><table class='members'><thead><tr><th>氏名</th><th>選挙区</th><th>役職</th></tr></thead><tbody><tr><td><a href='/member/tanaka'>田中太郎</a></td><td>東京1区</td><td>幹事長</td></tr><tr><td><a href='/member/yamada'>山田花子</a></td><td>大阪2区</td><td>政調会長</td></tr><tr><td><a href='/member/suzuki'>鈴木一郎</a></td><td>愛知3区</td><td>国対委員長</td></tr></tbody></table></body></html>",
            "page_number": null
          }
        ],
        "party_name": "サンプル党"
      },
      "expected_output": {
        "members": [
          {
            "name": "田中太郎",
            "position": "衆議院議員",
            "electoral_district": "東京1区",
            "prefecture": "東京都",
            "profile_url": "https://example-party.jp/member/tanaka",
            "party_position": "幹事長"
          },
          {
            "name": "山田花子",
            "position": "衆議院議員",
            "electoral_district": "大阪2区",
            "prefecture": "大阪府",
            "profile_url": "https://example-party.jp/member/yamada",
            "party_position": "政調会長"
          },
          {
            "name": "鈴木一郎",
            "position": "衆議院議員",
            "electoral_district": "愛知3区",
            "prefecture": "愛知県",
            "profile_url": "https://example-party.jp/member/suzuki",
            "party_position": "国対委員長"
          }
        ],
        "total_count": 3,
        "party_name": "サンプル党"
      },
      "evaluation_criteria": {
        "priority": "high",
        "difficulty": "easy",
        "notes": "Well-structured HTML table extraction with party positions"
      }
    },
